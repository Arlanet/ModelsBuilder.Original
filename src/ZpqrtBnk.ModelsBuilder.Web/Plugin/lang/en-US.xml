<?xml version="1.0" encoding="utf-8" ?>
<language alias="en_us" intName="English (US)" localName="English (US)" lcid="" culture="en-US">
  
  <!-- 
    as long as en-US is the default UI language (see Umbraco.Core.DefaultUILanguage 
    appSetting), all other languages will fall back to this one.
  -->
  
  <creator>
    <name>The Umbraco Community</name>
    <link>https://our.umbraco.com/</link>
  </creator>
  
  <!-- 'dashboardTabs' is the area alias used by Core for dashboard tabs -->
  <area alias="dashboardTabs">
    <!-- 'settingsModelsBuilder' is the dashboard alias, as per the injected manifest -->
    <key alias="settingsModelsBuilder">Models Builder</key>
  </area>
</language>