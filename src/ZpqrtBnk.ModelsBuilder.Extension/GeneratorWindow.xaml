<platformUi:DialogWindow x:Class="ZpqrtBnk.ModelsBuilder.Extension.GeneratorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZpqrtBnk.ModelsBuilder.Extension"
        xmlns:platformUi="clr-namespace:Microsoft.VisualStudio.PlatformUI;assembly=Microsoft.VisualStudio.Shell.15.0"
        mc:Ignorable="d"
        Title="Models Builder" Height="360" Width="640">
    <Grid>
        <Rectangle Fill="#FFdddddd" HorizontalAlignment="Stretch" Height="40" Margin="0,0,0,0" VerticalAlignment="Bottom" StrokeThickness="0" />
        <Button Content="Close" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="Close_OnClick" Height="20" Background="#FFeeeeee" Name="ButtonClose" />
        <ProgressBar HorizontalAlignment="Stretch" Height="20" Margin="10,0,10,50" VerticalAlignment="Bottom" Name="ProgressBar"/>
        <TextBox HorizontalAlignment="Stretch" Margin="10,10,10,80" VerticalAlignment="Stretch" Name="Text" Background="Transparent" BorderThickness="0" />

    </Grid>
</platformUi:DialogWindow>
