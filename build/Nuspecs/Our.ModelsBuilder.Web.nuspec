<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata minClientVersion="2.8.1">
    <id>Our.ModelsBuilder.Web</id>
    <version>$version$</version>
    <title>Our ModelsBuilder Web</title>
    <authors>The Umbraco Community</authors>
    <owners>The Umbraco Community</owners>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://raw.githubusercontent.com/ZpqrtBnk/Our.ModelsBuilder/v4/dev/etc/logo.png</iconUrl>
    <!--<icon>img\logo.png</icon>-->
    <description>Our ModelsBuilder Web Integration for Umbraco.</description>
    <projectUrl>https://github.com/ZpqrtBnk/Our.ModelsBuilder</projectUrl>
    <copyright>$copyright$</copyright>
    <language>en-US</language>
    <tags>umbraco</tags>
    <dependencies>
      <group targetFramework="net472">
        <dependency id="Our.ModelsBuilder" version="$version$" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!-- our dlls -->
    <file src="$solution$\build.tmp\bin\Our.ModelsBuilder.Web.dll" target="lib\net472\Our.ModelsBuilder.Web.dll" />
    <file src="$solution$\build.tmp\bin\Our.ModelsBuilder.Web.pdb" target="lib\net472\Our.ModelsBuilder.Web.pdb" />

    <!-- content for the dashboard -->
    <file src="$solution$\src\Our.ModelsBuilder.Web\Plugin\modelsbuilder.html" target="content\App_Plugins\Our.ModelsBuilder\modelsbuilder.html" />
    <file src="$solution$\src\Our.ModelsBuilder.Web\Plugin\modelsbuilder.controller.js" target="content\App_Plugins\Our.ModelsBuilder\modelsbuilder.controller.js" />
    <file src="$solution$\src\Our.ModelsBuilder.Web\Plugin\modelsbuilder.resource.js" target="content\App_Plugins\Our.ModelsBuilder\modelsbuilder.resource.js" />
    <file src="$solution$\src\Our.ModelsBuilder.Web\Plugin\lang\en-US.xml" target="content\App_Plugins\Our.ModelsBuilder\lang\en-US.xml" />
    <file src="$solution$\build\Nuspecs\logo.png" target="content\App_Plugins\Our.ModelsBuilder\logo.png" />

    <!-- transforms -->
    <file src="$solution$\build\Nuspecs\ModelsBuilder\web.config.install.xdt" target="content" />
    <file src="$solution$\build\Nuspecs\ModelsBuilder\web.config.uninstall.xdt" target="content" />

    <!-- the icon -->
    <!--<file src="$solution$\build\Nuspecs\logo.png" target="img\" />-->

  </files>
</package>