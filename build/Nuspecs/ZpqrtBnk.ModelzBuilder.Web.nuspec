<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata minClientVersion="2.8.1">
    <id>ZpqrtBnk.ModelzBuilder.Web</id>
    <version>$version$</version>
    <title>ZpqrtBnk ModelzBuilder Web</title>
    <authors>ZpqrtBnk</authors>
    <owners>ZpqrtBnk</owners>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://umbraco.com/dist/nuget/logo-small.png</iconUrl>
    <description>ZpqrtBnk ModelzBuilder Web Integration for Umbraco.</description>
    <projectUrl>https://github.com/zpqrtbnk/Zbu.ModelsBuilder</projectUrl>
    <copyright>$copyright$</copyright>
    <language>en-US</language>
    <tags>umbraco</tags>
    <dependencies>
      <group targetFramework="net472">
        <dependency id="ZpqrtBnk.ModelzBuilder" version="$version$" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!-- our dlls -->
    <file src="$solution$\build.tmp\bin\ZpqrtBnk.ModelzBuilder.Web.dll" target="lib\net472\ZpqrtBnk.ModelzBuilder.Web.dll" />
    <file src="$solution$\build.tmp\bin\ZpqrtBnk.ModelzBuilder.Web.pdb" target="lib\net472\ZpqrtBnk.ModelzBuilder.Web.pdb" />

    <!-- content for the dashboard -->
    <file src="$solution$\src\ZpqrtBnk.ModelzBuilder.Web\Dashboard\modelzbuilder.html" target="content\App_Plugins\ModelzBuilder\modelzbuilder.html" />
    <file src="$solution$\src\ZpqrtBnk.ModelzBuilder.Web\Dashboard\modelzbuilder.controller.js" target="content\App_Plugins\ModelzBuilder\modelzbuilder.controller.js" />
    <file src="$solution$\src\ZpqrtBnk.ModelzBuilder.Web\Dashboard\modelzbuilder.resource.js" target="content\App_Plugins\ModelzBuilder\modelzbuilder.resource.js" />
    <file src="$solution$\src\ZpqrtBnk.ModelzBuilder.Web\Dashboard\package.manifest" target="content\App_Plugins\ModelsBuilder\package.manifest" />

    <!-- transforms -->
    <file src="$solution$\build\nuspecs\ModelsBuilder\web.config.install.xdt" target="content" />
    <file src="$solution$\build\nuspecs\ModelsBuilder\web.config.uninstall.xdt" target="content" />

  </files>
</package>