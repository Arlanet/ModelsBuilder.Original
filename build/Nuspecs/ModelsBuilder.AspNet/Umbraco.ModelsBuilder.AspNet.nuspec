<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata minClientVersion="2.8.1">
    <id>Umbraco.ModelsBuilder.AspNet</id>
    <version>$version$</version>
    <title>Umbraco ModelsBuilder Site Integration</title>
    <authors>Umbraco HQ</authors>
    <owners>Umbraco HQ</owners>
    <licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>http://umbraco.com/media/357769/100px_transparent.png</iconUrl>
    <description>Umbraco ModelsBuilder Site Integration.</description>
    <projectUrl>https://github.com/zpqrtbnk/Zbu.ModelsBuilder</projectUrl>
    <copyright>$copyright$</copyright>
    <language>en-US</language>
    <tags>umbraco</tags>
    <dependencies>
      <!-- dont need that one as Umbraco.ModelsBuilder depends on it -->
      <!--
			<dependency id="Microsoft.CodeAnalysis.CSharp" version="0.6.4033103-beta" />
      -->
      <dependency id="Umbraco.ModelsBuilder" version="$version$" />
    </dependencies>
	</metadata>
	<files>
    <!-- our dlls -->
    <file src="Umbraco.ModelsBuilder.AspNet.dll" target="lib\Umbraco.ModelsBuilder.AspNet.dll" />
    <file src="Umbraco.ModelsBuilder.AspNet.pdb" target="lib\Umbraco.ModelsBuilder.AspNet.pdb" />

    <!-- content for the dashboard -->
    <file src="modelsbuilder.htm" target="content\App_Plugins\ModelsBuilder\modelsbuilder.htm" />
    <file src="modelsbuilder.controller.js" target="content\App_Plugins\ModelsBuilder\modelsbuilder.controller.js" />
    <file src="package.manifest" target="content\App_Plugins\ModelsBuilder\package.manifest" />

    <!-- post-install script // we don't have one -->
    <!--
		<file src="tools\install.ps1" target="tools\install.ps1" />
		-->
    
    <!-- transforms -->
    <file src="web.config.install.xdt" target="content" />
    <file src="web.config.uninstall.xdt" target="content" />
    <file src="dashboard.config.install.xdt" target="content\config" />
    <file src="dashboard.config.uninstall.xdt" target="content\config" />

  </files>
</package>